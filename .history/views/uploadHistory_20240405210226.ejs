<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Upload History</title>
</head>
<body>
    <h1>Upload History</h1>
    <% if (history.length > 0) { %>
        <table>
            <thead>
                <tr>
                    <th>PDF Path</th>
                    <th>XLSX Path</th>
                    <th>Timestamp</th>
                </tr>
            </thead>
            <tbody>
                <% history.forEach(item => { %>
                    <tr>
                        <td><%= item.Pdf_path %></td>
                        <td><%= item.xlsx_path %></td>
                        <td><%= item.timestamp %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    <% } else { %>
        <p>No upload history available.</p>
    <% } %>
</body>
</html>
