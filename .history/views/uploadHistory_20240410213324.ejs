<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Upload History</title>
    <style>
        table {
            width: 100%;
            border-collapse: collapse;
        }
        th, td {
            border: 1px solid #ddd;
            padding: 8px;
            text-align: left;
        }
        th {
            background-color: #f2f2f2;
        }
    </style>
</head>
<body>
    <div class="container">
        <a href="/">
            <button>Back</button>
        </a>
        <h1>Upload History</h1>
        <table>
            <thead>
                <tr>
                    <th>S. No</th>
                    <th>Date</th>
                    <th>Uploaded By</th>
                </tr>
            </thead>
            <tbody>
                <% history.sort((a, b) => a.timestamp - b.timestamp).forEach((item, index) => { %>
                    <tr>
                        <td><%= index + 1 %></td>
                        <td><%= new Date(item.timestamp).toLocaleString() %></td>
                        <td><%= item.uploaded_By %></td>
                    </tr>
                <% }); console.log(%>
            </tbody>
        </table>
    </div>
</body>
</html>
